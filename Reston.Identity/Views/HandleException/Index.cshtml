@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ThemeBucket">
    <link rel="shortcut icon" href="#" type="image/png">

    <title>Eror Page</title>

    <link href="@Url.Content("/Content/handle/style.css")" rel="stylesheet" />
    <link href="@Url.Content("/Content/handle/style-responsive.css")" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body class="error-page">

    <section>
        <div class="container ">

            <section class="error-wrapper text-center">

                
                <h2>OOOPS!!!</h2>
                <b>
                    <h4 style="color:black">
                        @ViewBag.RedirectReason @if (this.User.Identity.IsAuthenticated)
                        { <a style="color:blue" href="@Url.Action("Signout", "Header")">Silahkan Logout </a>}
                    </h4>
                </b>

            </section>

        </div>
    </section>

    <!-- Placed js at the end of the document so the pages load faster -->

    <script src="@Url.Content("/Scripts/handle/jquery-1.10.2.min.js")"></script>
    <script src="@Url.Content("/Scripts/handle/jquery-migrate-1.2.1.min.js")"></script>
    <script src="@Url.Content("/Scripts/handle/bootstrap.min.js")"></script>
    <script src="@Url.Content("/Scripts/handle/modernizr.min.js")"></script>

    <!--common scripts for all pages-->
    <!--<script src="js/scripts.js"></script>-->

</body>
</html>
